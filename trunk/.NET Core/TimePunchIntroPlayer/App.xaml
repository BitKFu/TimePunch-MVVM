<Application
    x:Class="TimePunchIntroPlayer.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="clr-namespace:TimePunchIntroPlayer.Converters"
    xmlns:local="clr-namespace:TimePunchIntroPlayer"
    StartupUri="Windows/MainWindow.xaml">
    <Application.Resources>
        <converter:DoubleToIntConverter x:Key="DoubleToIntConverter" />
        <converter:DoubleToTimespanConverter x:Key="DoubleToTimespanConverter" />
        <local:ViewModelLocater x:Key="ViewModelLocater" />
    </Application.Resources>
</Application>
